\n  return <PackMild />\r\n}\r\n\r\nconst PackCold = () => {\r\n  return(\r\n    <ul>\r\n    <li>Jacket</li>\r\n    <li>Pants</li>\r\n    <li>Scarf</li>\r\n    </ul>\r\n  )\r\n}\r\n\r\nconst PackMild = () => {\r\n  return(\r\n    <ul>\r\n    <li>Jumper</li>\r\n    <li>Pants</li>\r\n    <li>Hat</li>\r\n    </ul>\r\n  )\r\n}\r\n\r\nconst PackHot = () => {\r\n  return(\r\n    <ul>\r\n    <li>Singlet</li>\r\n    <li>Shorts</li>\r\n    </ul>\r\n  )\r\n}\r\n\r\nrender(<App />, document.querySelector('#app'))\r\n"},"lineCount":null}},"error":null,"hash":"3628d801af8d8d135ac57417a02ae3f3","cacheData":{"env":{}}}